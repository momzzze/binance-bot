# Binance Spot Testnet Bot - Environment Example
# Copy to .env and fill values

PORT=3000
# Core
SYMBOLS="BTCUSDT,ETHUSDT"
INTERVAL="1m"
TRADING_ENABLED=false
BOT_KILL_SWITCH=false
MAX_ORDER_USDT=20
MAX_OPEN_ORDERS_PER_SYMBOL=1
LOOP_MS=5000

# Binance API
BINANCE_BASE_URL="https://testnet.binance.vision"
BINANCE_API_KEY=""
BINANCE_API_SECRET=""

# Database
POSTGRES_URL="postgres://postgres:postgres@localhost:5432/binance_bot"
